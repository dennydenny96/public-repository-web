doctype html
html
  head
    include ../header.pug

  body.hold-transition.skin-blue

    //- Main content
    section.content     
      .row
        .col-sm-12(style="max-width:951px;")
          table#pageTable.table.dataTable.table-condensed.table-bordered.table-striped.table-hover.nowrap(style="width:900px;")
            thead
              tr
                th Title
                th Price
                th Loan Day
                th Return Day
                th Action

    include ../footerAlert.pug

    //- Script
    include ../script.pug
    script.
    script(src="/dist/js/transactionsLoanBookList"+locals.minify+".js?v=" + locals.vjs)

    //- Modal
    include ../loanbook/transactionLoanBookAdd.pug
    include ../modalConfirm.pug